<%= render "head/header"%>
<section class="row">
  <%if current_user.admin?%>
    <%=render 'admin/nav_left'%>
  <%else%>
    <%= render "nav_left" %>
  <%end%>
  <div class="col-xl-8 nav-bw px-0 nav_bww">
    <h4>Edit Photo</h4>
    <%= render "form_photo", photo: @photo %>
    <%# <div class=> %>
    <%# <%= button_to "Save", user_path(current_user.id), method: :get, class: "save"%>
    <div class="del">
      <%= button_to "Destroy this photo", @photo, method: :delete, data:{confirm: "Are you sure?" },class: "destroy"%>
    </div>
  </div>
  <%if current_user.admin?%>
    <%=render 'admin/nav_right'%>
  <%else%>
    <%= render "nav_right" %>
  <%end%>
  <footer>
  </footer>
</section>
